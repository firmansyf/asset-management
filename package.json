    "eslint-plugin-sonar": "^0.12.0",
    "eslint-plugin-sonarjs": "^0.20.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.1.6",
    "webpack-cli": "^5.1.4"
