  "scripts": {
    "start": "craco start",
    "build": "GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true craco --max_old_space_size=2048 build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test --env=jsdom",
    "cy": "npx cypress open --config-file cypress.local.json",
    "cy:run": "npx cypress run --config-file cypress.local.json --record --key 9933cca6-98fa-40c5-ada6-0d7c482304cb",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run --record --key 9933cca6-98fa-40c5-ada6-0d7c482304cb",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:quiet": "eslint ./src --quiet",
    "lint:strict": "eslint ./src --fix --max-warnings=0",
    "lint:report": "eslint ./src -f node_modules/eslint-detailed-reporter/lib/detailed.js -o public/eslint-report.html",
    "format": "prettier --write ./src",
    "prepare": "husky install",
    "pkg": "npx depcheck --config=.depcheckrc"
  },
  "yarn-upgrade-all": {
    "ignore": []
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
